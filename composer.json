{
    "name": "geeshoe/helpers",
    "description": "Helper utilities for PHP projects",
    "type": "library",
    "license": "Apache-2.0",
    "homepage": "http://www.geeshoe.com",
    "authors": [
        {
            "name": "Jesse Rushlow",
            "email": "jr@geeshoe.com"
        }
    ],
    "require": {
        "php": ">=7.1",
        "ramsey/uuid": "^3.8"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.1",
        "mikey179/vfsstream": "^1.6",
        "squizlabs/php_codesniffer": "3.*",
        "roave/security-advisories": "dev-master",
        "phpstan/phpstan": "^0.12.3",
        "phpstan/extension-installer": "^1.0",
        "phpstan/phpstan-phpunit": "^0.12.3"
    },
    "autoload": {
        "psr-4": {
            "Geeshoe\\Helpers\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Geeshoe\\HelpersTest\\": "tests"
        }
    }
}
